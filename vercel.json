{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": { 
          "distDir": "build"
        }
      },
      {
        "src": "backend/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/api/$1"
      },
      {
        "src": "/static/(.*)",
        "dest": "/frontend/build/static/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/frontend/build/favicon.ico"
      },
      {
        "src": "/manifest.json",
        "dest": "/frontend/build/manifest.json"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/index.html"
      }
    ]
  }
  
  
  
  
  
  
  