{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": { 
          "distDir": "build"
        }
      },
      {
        "src": "backend/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/[^.]+",
        "dest": "/frontend/build/index.html",
        "status": 200
      }
    ]
  }
  
  
  
  
  
  
  